{
   // Use IntelliSense to find out which attributes exist for C# debugging
   // Use hover for the description of the existing attributes
   // For further information visit https://github.com/OmniSharp/omnisharp-vscode/blob/master/debugger-launchjson.md
   "version": "0.2.0",
   "configurations": [
   {
       "name": ".NET Core Launch (openid client)",
       "type": "coreclr",
       "request": "launch",
       "preLaunchTask": "build openidclient",
       "program": "${workspaceFolder}/src/Ataoge.SsoServer.OpenIdClient/bin/Debug/netcoreapp3.0/Ataoge.SsoServer.OpenIdClient.dll",
       "args": [],
       "cwd": "${workspaceFolder}/src/Ataoge.SsoServer.OpenIdClient",
       "stopAtEntry": false,
       "serverReadyAction": {
           "action": "openExternally",
           "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"
       },
       "env": {
           "ASPNETCORE_ENVIRONMENT": "Development"
       },
       "sourceFileMap": {
           "/Views": "${workspaceFolder}/src/Ataoge.SsoServer.OpenIdClient/Views"
       }
   },
        {
            "name": ".NET Core Launch (web)",
            "type": "coreclr",
            "request": "launch",
            "preLaunchTask": "build",
            // If you have changed target frameworks, make sure to update the program path.
            "program": "${workspaceFolder}/src/Ataoge.SsoServer.Web/bin/Debug/netcoreapp3.0/Ataoge.SsoServer.Web.dll",
            "args": [],
            "cwd": "${workspaceFolder}/src/Ataoge.SsoServer.Web",
            "stopAtEntry": false,
            // Enable launching a web browser when ASP.NET Core starts. For more information: https://aka.ms/VSCode-CS-LaunchJson-WebBrowser
            "serverReadyAction": {
                "action": "openExternally",
                "pattern": "^\\s*Now listening on:\\s+(https?://\\S+)"                
            },
            "env": {
                "ASPNETCORE_ENVIRONMENT": "Development"
            },
            "sourceFileMap": {
                "/Views": "${workspaceFolder}/src/Ataoge.SsoServer.Web/Views"
            }
        },
        {
            "name": ".NET Core Attach",
            "type": "coreclr",
            "request": "attach",
            "processId": "${command:pickProcess}"
        }
    ]
}